import java.util.Scanner;

public class IsAgeChecker {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Введите возраст.");
		int age = sc.nextInt();
		if (age <= 0 || age > 120) {
			System.out.println("false");
		} else {
			System.out.println("true");
		}
	}
}
