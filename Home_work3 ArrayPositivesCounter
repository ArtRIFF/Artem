public class ArrayPositivesCounter {

	public static void main(String[] args) {
		int[] b = { -1, 3, -2, -10, 1 };
		System.out.println(countPositives(b));
	}
	private static int countPositives(int[]a) {
		int count=0;
		for (int i = 0; i < a.length; i++) {
			if(a[i]>0){
				count++;
			}
		}
		return count;
	}
}
