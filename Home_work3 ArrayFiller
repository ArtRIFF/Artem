mport java.util.Arrays;
public class ArrayFiller {
	
public static void main(String[] args) {
	int [] a={4,4,5,63,5};
	arrayFiller(a, 20);
	System.out.println(Arrays.toString(a));
			
}
public static  void arrayFiller(int[] array,int filler) {
	for (int i = 0; i < array.length; i++) {
		array[i]=filler;
	}
}
}
